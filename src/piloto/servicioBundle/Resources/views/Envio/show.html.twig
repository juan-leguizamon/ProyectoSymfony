<h1>Envio</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Idformasenvio</th>
            <td>{{ entity.idFormasEnvio }}</td>
        </tr>
        <tr>
            <th>Idformulario</th>
            <td>{{ entity.idFormulario }}</td>
        </tr>
        <tr>
            <th>Idestado</th>
            <td>{{ entity.idEstado }}</td>
        </tr>
        <tr>
            <th>Idmensajero</th>
            <td>{{ entity.idMensajero }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('envio') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('envio_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('envio_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
