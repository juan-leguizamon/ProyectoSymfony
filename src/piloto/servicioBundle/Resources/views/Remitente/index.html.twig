<h1>Remitente list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Remitente</th>
            <th>Empresaasociada</th>
            <th>Nombre</th>
            <th>Cedula</th>
            <th>Direccion</th>
            <th>Pais</th>
            <th>Ciudad</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('remitente_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.remitente }}</td>
            <td>{{ entity.empresaAsociada }}</td>
            <td>{{ entity.nombre }}</td>
            <td>{{ entity.cedula }}</td>
            <td>{{ entity.direccion }}</td>
            <td>{{ entity.pais }}</td>
            <td>{{ entity.ciudad }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('remitente_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('remitente_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('remitente_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
