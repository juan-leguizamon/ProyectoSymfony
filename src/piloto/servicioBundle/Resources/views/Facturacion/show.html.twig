<h1>Facturacion</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Idenvio</th>
            <td>{{ entity.idEnvio }}</td>
        </tr>
        <tr>
            <th>Firmadestinatario</th>
            <td>{{ entity.firmaDestinatario }}</td>
        </tr>
        <tr>
            <th>Firmamensajero</th>
            <td>{{ entity.firmaMensajero }}</td>
        </tr>
        <tr>
            <th>Costoenvio</th>
            <td>{{ entity.costoEnvio }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('facturacion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('facturacion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('facturacion_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
