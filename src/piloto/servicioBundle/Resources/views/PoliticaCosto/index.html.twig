<h1>PoliticaCosto list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Tamaño</th>
            <th>Peso</th>
            <th>Volumen</th>
            <th>Pais</th>
            <th>Ciudad</th>
            <th>Cantidad</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('politica_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.tamaño }}</td>
            <td>{{ entity.peso }}</td>
            <td>{{ entity.volumen }}</td>
            <td>{{ entity.pais }}</td>
            <td>{{ entity.ciudad }}</td>
            <td>{{ entity.cantidad }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('politica_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('politica_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('politica_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
