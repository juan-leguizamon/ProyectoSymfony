<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PoliticaCosto" table="politica_costo">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tamaño" type="float" column="Tamaño" nullable="false"/>
    <field name="peso" type="float" column="Peso" nullable="false"/>
    <field name="volumen" type="float" column="Volumen" nullable="false"/>
    <field name="pais" type="string" column="Pais" length="12" nullable="false"/>
    <field name="ciudad" type="string" column="Ciudad" length="12" nullable="false"/>
    <field name="cantidad" type="integer" column="Cantidad" nullable="false"/>
  </entity>
</doctrine-mapping>
