<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Formulario" table="formulario">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idDestinatario" type="integer" column="ID_Destinatario" nullable="false"/>
    <field name="idTipoRemitente" type="integer" column="ID_Tipo_Remitente" nullable="false"/>
    <field name="idFormaPago" type="integer" column="ID_Forma_Pago" nullable="false"/>
    <field name="idPaquete" type="integer" column="ID_Paquete" nullable="false"/>
  </entity>
</doctrine-mapping>
